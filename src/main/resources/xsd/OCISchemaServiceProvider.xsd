          <xs:element name="provisioningSliceId" type="SliceId" minOccurs="0"/>
          <xs:element name="subscriberPartition" type="SubscriberPartition" minOccurs="0"/>
          <xs:element name="preferredDataCenter" type="DataCenter" minOccurs="0"/>
          <xs:element name="resellerId" type="ResellerId22" minOccurs="0"/>
        </xs:sequence>
      </xs:extension>
    </xs:complexContent>
  </xs:complexType>

